{
  "name": "@tybys/bommon",
  "version": "2.0.0",
  "description": "CommonJS in Browser",
  "scripts": {
    "build": "uglifyjs bommon.js --ie8 -m -o bommon.min.js && uglifyjs bommon.minimal.js --ie8 -m -o bommon.minimal.min.js",
    "build:path": "uglifyjs lib/path.js --ie8 -m -o lib/path.min.js",
    "build:events": "uglifyjs lib/events.js --ie8 -m -o lib/events.min.js",
    "build:querystring": "uglifyjs lib/querystring.js --ie8 -m -o lib/querystring.min.js",
    "build:buffer": "uglifyjs lib/buffer.js --ie8 -m -o lib/buffer.min.js"
  },
  "author": "toyobayashi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toyobayashi/bommon.git"
  },
  "license": "MIT",
  "keywords": [
    "cjs"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.3.0",
    "uglify-js": "^3.7.3"
  }
}
